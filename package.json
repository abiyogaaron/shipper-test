    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:test": "GENERATE_SOURCEMAP=true node -r dotenv/config ./node_modules/.bin/react-scripts build dotenv_config_path=.env.test",
